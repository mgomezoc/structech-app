{
  "name": "structech-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:apk": "npm run build && npm run sync && cd android && ./gradlew :app:assembleDebug && cd ..",
    "preview": "vite preview",
    "serve": "npx live-server www",
    "sync": "npm run build && npx cap sync",
    "android": "npx cap open android",
    "run:android": "npm run sync && npx cap run android",
    "clean": "rm -rf www node_modules/.vite",
    "icons:ios": "cordova-res ios --skip-config --copy --copy-to ios/App/App/Assets.xcassets --icon-only",
    "icons:android": "cordova-res android --skip-config --copy --copy-to android/app/src/main/res --icon-only",
    "icons:all": "npm run icons:ios && npm run icons:android",
    "resources:ios": "cordova-res ios --skip-config --copy --copy-to ios/App/App/Assets.xcassets",
    "resources:android": "cordova-res android --skip-config --copy --copy-to android/app/src/main/res",
    "resources": "npm run resources:android && npm run resources:ios",
    "build:full": "npm run clean && npm run icons:all && npm run build && npm run sync",
    "ios:dev": "npm run icons:ios && npm run sync && npx cap open ios",
    "android:dev": "npm run icons:android && npm run sync && npx cap open android",
    "dev:quick": "npm run build && npm run sync",
    "fix-adaptive": "node update-adaptive-icons.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aparajita/capacitor-biometric-auth": "^9.0.0",
    "@aparajita/capacitor-secure-storage": "^6.0.1",
    "@capacitor/app": "^7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/core": "^7.3.0",
    "@capacitor/dialog": "^7.0.1",
    "@capacitor/geolocation": "^7.1.2",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@capacitor/toast": "^7.0.1",
    "@ionic/pwa-elements": "^3.3.0",
    "@lordicon/element": "^1.11.0",
    "@microblink/blinkid-capacitor": "^6.13.1",
    "axios": "^1.10.0",
    "capacitor-voice-recorder": "^7.0.6",
    "handlebars": "^4.7.8",
    "lottie-web": "^5.13.0",
    "mitt": "^3.0.1",
    "navigo": "^8.11.1",
    "signature_pad": "^5.0.10"
  },
  "devDependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/cli": "^7.3.0",
    "cordova-res": "^0.15.4",
    "less": "^4.3.0",
    "postcss-preset-env": "^7.8.3",
    "vite": "^6.3.5",
    "vite-plugin-handlebars": "^2.0.0"
  }
}
